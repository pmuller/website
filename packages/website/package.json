{
  "name": "website",
  "packageManager": "yarn@3.2.0",
  "scripts": {
    "format": "prettier --write .",
    "build": "gatsby build",
    "check:eslint": "eslint --max-warnings 0 ./src",
    "check:prettier": "prettier --check .",
    "check:stylelint": "stylelint src/**/*.tsx gatsby-browser.js",
    "check": "yarn check:prettier && yarn check:stylelint && yarn check:eslint",
    "clean": "rm -rf ./public",
    "server": "gatsby develop",
    "test": "echo No tests"
  },
  "dependencies": {
    "@babel/core": "^7.17.5",
    "@mdi/js": "^6.5.95",
    "@mdi/react": "^1.5.0",
    "@svgr/webpack": "^6.2.1",
    "gatsby": "^4.8.0",
    "gatsby-plugin-image": "^2.8.1",
    "gatsby-plugin-linaria": "^3.2.0",
    "gatsby-plugin-sharp": "^4.8.1",
    "gatsby-plugin-svgr-svgo": "^2.0.0",
    "gatsby-source-filesystem": "^4.8.1",
    "gatsby-transformer-sharp": "^4.8.0",
    "linaria": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.9.0",
    "eslint-config-typescript": "workspace:^",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-sonarjs": "^0.12.0",
    "prettier": "^2.5.1",
    "prettier-config": "workspace:^",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0",
    "typescript": "^4.5.5"
  },
  "prettier": "prettier-config",
  "sideEffects": false
